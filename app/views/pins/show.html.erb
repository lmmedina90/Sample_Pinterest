<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @pin.name %>
</p>

<p>
  <strong>Image:</strong>
  <%= image_tag(@pin.image_url, :width => 400) if @pin.image.present? %>
</p>

<p>
  <strong>User:</strong>
  <%= @pin.user.full_name %>
</p>

<p>
  <strong>Board:</strong>
  <%= @pin.board.name %>
</p>

<div id="navbar2">
  <table>
    <td><%= link_to 'Edit', edit_pin_path(@pin) %></td>
    <td><%= link_to 'Back', pins_path %></td>
  </table>
</div>
